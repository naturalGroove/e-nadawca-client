{
  "name": "naturalgroove/e-nadawca-client",
  "type": "library",
  "description": "simple soap client for https://e-nadawca.poczta-polska.pl/",
  "license": "MIT",
  "keywords": [
    "php",
    "e-nadawca",
    "poczta-polska"
  ],
  "replace": {
    "abryb/e-nadawca-client": "self.version"
  },
  "authors": [
    {
      "name": "Błażej Rybarkiewicz",
      "email": "b.rybarkiewicz@cogitech.pl"
    },
    {
      "name": "Maciej Kobus",
      "email": "webhdx@gmail.com"
    },
    {
      "name": "Grzegorz Adamczyk",
      "email": "grzegorz@netseven.pl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Abryb\\ENadawca\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Abryb\\ENadawca\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-soap": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "goetas-webservices/xsd2php": "^0.4.12",
    "phpunit/phpunit": "^10.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
